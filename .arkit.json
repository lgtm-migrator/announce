{
	"components": [{
			"type": "Dependency",
			"patterns": [
				"node_modules/*"
			]
		},
		{
			"type": "Component",
			"patterns": [
				"**/*.js"
			]
		}
	],
	"excludePatterns": [
		"**/.circleci/",
		"**/.git/",
		"**/.github/",
		"**/buildresults/",
		"**/coverage/",
		"**/docs/",
		"**/jsdoc_default/",
		"**/logs/"
	],
	"output": [{
		"path": [
			"arkit/announce.svg"
		],
		"groups": [{
				"first": true,
				"components": [
					"Component"
				]
			},
			{
				"type": "Dependencies",
				"components": [
					"Dependency"
				]
			}
		]
	}]
}
